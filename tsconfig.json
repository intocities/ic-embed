{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": false,
    "target": "es5",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "lib": ["es5", "dom"],
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": ["./src/**/*"]
}
